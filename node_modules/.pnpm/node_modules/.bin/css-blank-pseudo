#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/ddyy/Desktop/Flowbite Pro Admin Dashboard (HTML) v2.1.0/node_modules/.pnpm/css-blank-pseudo@0.1.4/node_modules/css-blank-pseudo/node_modules:/mnt/c/Users/ddyy/Desktop/Flowbite Pro Admin Dashboard (HTML) v2.1.0/node_modules/.pnpm/css-blank-pseudo@0.1.4/node_modules:/mnt/c/Users/ddyy/Desktop/Flowbite Pro Admin Dashboard (HTML) v2.1.0/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/ddyy/Desktop/Flowbite Pro Admin Dashboard (HTML) v2.1.0/node_modules/.pnpm/css-blank-pseudo@0.1.4/node_modules/css-blank-pseudo/node_modules:/mnt/c/Users/ddyy/Desktop/Flowbite Pro Admin Dashboard (HTML) v2.1.0/node_modules/.pnpm/css-blank-pseudo@0.1.4/node_modules:/mnt/c/Users/ddyy/Desktop/Flowbite Pro Admin Dashboard (HTML) v2.1.0/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../css-blank-pseudo/cli.js" "$@"
else
  exec node  "$basedir/../css-blank-pseudo/cli.js" "$@"
fi
